return obj {
    public any errors = 0;

    public any total = 0;

    public any assert = f(val, expected, text = '') {
        any icon = '✓';
        any color = "\x1b[32m ";
        any reset = "\x1b[0m ";
        if (val !== expected) {
            icon = '✕';
            color = "\x1b[31m ";
            this.errors++;
        }

        this.total++;
        printLine(color ^ icon ^ reset ^ text);
    };
};
